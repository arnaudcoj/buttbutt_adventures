script/source = "extends Node

onready var body = get_parent()

export var jump_height = 40.0
export var time_to_jump_apex = 0.4
export var acceleration_time_airborne = 0.2
export var acceleration_time_grounded = 0.1
export var move_speed = 600

var gravity = 0.0
var jump_velocity = 0.0
var velocity = Vector2()

func _ready():
	gravity = (2.0 * jump_height) / pow(time_to_jump_apex, 2.0)
	jump_velocity = abs(gravity) * time_to_jump_apex
	
func _physics_process(delta):
	if body.collision_info.below or body.collision_info.above:
		velocity.y = 0
	else:
		velocity.y += gravity * delta

	var input = Vector2()
	
	if Input.is_action_pressed(\"ui_left\") and not body.collision_info.left :
		input.x -= 1
	if Input.is_action_pressed(\"ui_right\") and not body.collision_info.right :
		input.x += 1
	
	if Input.is_action_pressed(\"ui_up\") and body.collision_info.below:
		velocity.y = -jump_velocity
		
	var target_velocity_x = input.x * move_speed
	
	velocity.x = lerp(velocity.x, target_velocity_x, acceleration_time_grounded if body.collision_info.below else acceleration_time_airborne) # use actual smoothdamp 
	
#	print(velocity)
	
	body.move(velocity * delta)"

